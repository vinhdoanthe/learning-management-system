<h1>Redeem Redeem Product Categories</h1>

<div class="table-responsive">
<table class="table table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @adm_redeem_redeem_product_categories.each do |adm_redeem_redeem_product_category| %>
      <% unless adm_redeem_redeem_product_category.nil? %>
        <tr>
          <th><%= adm_redeem_redeem_product_category.id %></th>
          <td><%= adm_redeem_redeem_product_category.name %></td>
          <td><%= link_to 'Edit', edit_adm_redeem_redeem_product_category_path(adm_redeem_redeem_product_category), class: 'btn btn-warning' %></td>
          <td><%= link_to 'Destroy', adm_redeem_redeem_product_category_path(adm_redeem_redeem_product_category), class: 'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Redeem Redeem Product Category', new_adm_redeem_redeem_product_category_path, class: 'btn btn-primary' %>

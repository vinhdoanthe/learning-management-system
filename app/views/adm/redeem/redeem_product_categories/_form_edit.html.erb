<%= form_with(url: adm_redeem_redeem_product_category_path(adm_redeem_redeem_product_category), local: true, method: :patch, class: 'form-horizontal') do |form| %>

  <div class="form-group row">
    <label for="brand_name" class="col-sm-2 col-form-label">Name</label>
    <div class="col-sm-10">
      <%= form.text_field :name, class: "form-control", value: adm_redeem_redeem_product_category.name %>
    </div>
  </div>

  <div class="actions">
    <%= form.submit(class: 'btn btn-success') %>
  </div>
<% end %>

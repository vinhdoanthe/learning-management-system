res = JSON.parse('<%= raw(result.to_json) %>');
display_response_noti(res);

<% if result[:type] == 'success' %>
    <% topic = result[:topic] %>
    <% contest = result[:contest] %>
    $('#create-new-contest-topic').html("<%= escape_javascript( render partial: 'adm/contest/contest_topics/partials/contest_topic_detail', locals: { contest: contest, topic: Contest::ContestTopic.new } ) %> ")
    $("#tbody-contest-topic").append(`
      <tr>
      <td style="widtd: 10px"><%= topic.id %></td>
        <td><%= topic.name %></td>
        <td><%= topic.contest.name %></td>
        <td><%= topic.region %></td>
        <td><%= topic&.description.html_safe %></td>
        <td><%= topic&.rule.html_safe %></td>
        <td>
        <% if topic.thumbnail.attached? %> <%= image_tag topic.thumbnail, style: "widtd: 20px; height: 20px" %>
        <% end %>
        </td>
      </tr>
      `);


    $(".table-contest-topic").append(`
      <tr>
      <td style="widtd: 10px"><%= topic.id %></td>
        <td><%= topic.name %></td>
        <td><%= topic.contest.name %></td>
        <td><%= topic.region %></td>
        <td><%= topic&.description.html_safe %></td>
        <td><%= topic&.rule.html_safe %></td>
        <td>
        <% if topic.thumbnail.attached? %> <%= image_tag topic.thumbnail, style: "widtd: 20px; height: 20px" %>
        <% end %>
        </td>
      </tr>
      `);
<% end %>

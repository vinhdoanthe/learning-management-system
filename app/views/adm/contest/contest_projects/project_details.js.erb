$('#studentProjectDetail').html("<%= escape_javascript( render partial: 'adm/contest/contest_projects/modals/marking_project', locals: { student: student, c_project: c_project, project: project, course: course, batch: batch, company_name: company_name, like: like, share: share, topics: topics, contest: contest, topic: topic }) %>")

$(document).ready(function(){
  $('#update_topic_for_project').on('click', function(){
    project_id = $(this).data('project');
    topic_id = $('#project_topic_choose').val()

    $.ajax({
      method: 'POST',
      url: '/adm/contest/contest_projects/update',
      data: { project_id: project_id, contest_topic_id: topic_id },
      success: function(res){
        display_response_noti(res)
      }
    })
  })
})


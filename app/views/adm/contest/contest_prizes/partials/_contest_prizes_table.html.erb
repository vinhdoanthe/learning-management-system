<div class="card table-responsive">
  <div class='card-hearder'>
  </div>
  <div class="card-body">

    <table id="contest_prize_list" style="width: 100%">
      <thead>
        <tr>
          <th>#</th>
          <th>name</th>
          <th>student_price</th>
          <th>teacher_price</th>
          <th>number_reward</th>
          <th>order_prize</th>
          <th>type</th>
          <th>total</th>
          <th>description</th>
        </tr>
      </thead>
      <tbody id="table-prize-body">
        <%= prizes.each do |prize| %>
          <tr>
            <td><%= prize.id %></td>
            <td><%= prize.name %></td>
            <td><%= prize.student_price %></td>
            <td><%= prize.teacher_price %></td>
            <td><%= prize.number_awards %></td>
            <td><%= prize.prize %></td>
            <td><%= t("adm.contest.contest_prizes.#{ prize.prize_type }") %></td>
            <td><%= prize.student_price + prize.teacher_prize %></td>
            <td><%= prize.description %></td>
          </tr>
        <% end%>
    </table>
  </div>
</div>

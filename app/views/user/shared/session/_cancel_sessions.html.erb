<div class="table-responsive bo-table">
  <div class="fc-header">
    <h3>Buổi học <span class="color-F16357">đã hủy</span></h3>
  </div>

  <% if !cancel_sessions.nil? and !cancel_sessions.empty? %>
    <table class="table  table-link">
      <thead>
      <tr>
        <th width="90">&nbsp;</th>
        <th width="140"></th>
        <th></th>
        <th></th>
      </tr>
      </thead>
      <tbody>
      <% cancel_sessions.each do |cancel_session| %>
        <%= render 'user/shared/session/row/cancel_session_student', {:session => cancel_session} %>
      <% end %>
      </tbody>
    </table>
  <% else %>
    <div>Không có buổi học nào bị hủy</div>
  <% end %>
</div>
$("#homework_batch_selection").html("<%= escape_javascript(render partial: 'user/op_students/partials/homework_selection', locals: { datas: batches } ) %>");
$("#homework_subject_selection").html("<%= escape_javascript(render partial: 'user/op_students/partials/homework_selection', locals: { datas: subjects } ) %>");
$("#homework_session_table").html("<%= escape_javascript(render partial: 'user/op_students/partials/table_homework_list', locals: { sessions: sessions } ) %>");
$('#homework_course_selection').val("<%= course.id %>")
$('#homework_batch_selection').val("<%= batch.id %>")
$('#homework_subject_selection').val("<%= subject.id %>")

$(document).on('click', '#student_homework_videos', function(){
  $("#homework_video_detail").html("<%= escape_javascript(render partial: 'user/op_students/partials/homework_video_detail', locals: {course: course, batch: batch, session: session, subject: subject} ) %>")
  $("#student_videos_list").html("<%= escape_javascript(render partial: 'user/op_students/partials/student_videos_list', locals: { sessions: sessions, session: session, subject: subject } ) %>");

  $($('input[name="session_homework_name"][value="<%= session.id %>"]')[1]).parent().addClass('undone_question')
})

  <% if show_video %>
    $('#student_homework_videos').trigger('click')
<% end %>

  $(document).on('click', '.student_homework_sesison', function(){
    $("#homework_video_detail").html("<%= escape_javascript(render partial: 'user/op_students/partials/homework_video_detail', locals: {course: course, batch: batch, subject: subject} ) %>")
  })

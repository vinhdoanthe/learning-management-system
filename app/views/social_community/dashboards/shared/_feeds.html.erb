<!-- Variables: feeds, next_time_offset -->
<% if feeds.present? %>
  <% feeds.each do |feed| %>
    <% if feed.post.is_a?(SocialCommunity::Feed::RewardPost) %>
      <% message, reward = reward_feed(feed) %>
      <%= render :partial => 'social_community/feed/reward_posts/reward_post', :locals => {feed: feed, message: message.html_safe, reward: reward} %>
    <% elsif feed.post.is_a?(SocialCommunity::Feed::PhotoPost) %>
      <% message, photos = photo_feed(feed) %>
      <%= render :partial => 'social_community/feed/photo_posts/photo_post', :locals => {feed: feed, message: message.html_safe, photos: photos} %>
    <% end %>
  <% end %>
<% end %>

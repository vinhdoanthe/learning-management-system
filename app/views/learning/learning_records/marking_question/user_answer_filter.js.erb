<% if user_answers.blank? %>
  $('#user_answer_list').html('<p>Lớp học này chưa có bài tập cần chấm!</p>')
<% else %>
  $('#user_answer_list').html("<%= escape_javascript(render partial: 'learning/learning_records/marking_question/user_answer_list', locals: { user_answers: user_answers, batch: batch } ) %>");
  <% answer_ids = user_answers.map{ |answer| answer[:answer_id] } %>
  $('input[name="user_answer_ids"]').val("<%= answer_ids.to_json %>");
<% end %>

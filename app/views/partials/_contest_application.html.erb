<!DOCTYPE html>
<html>

  <head>
    <title>TEKY Learning Management System</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag 'Logo-fav.ico' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolink.mCustomScrollBars-track': 'reload' %>
    <%= stylesheet_link_tag 'contest', media: 'all', 'data-turbolink.mCustomScrollBars-track': 'reload' %>
    <%= javascript_include_tag 'contest', 'data-turbolinks-track': 'reload' %>
    <div id="fb-root"></div>
<script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v8.0&appId=332989550981679&autoLogAppEvents=1" nonce="YeBzg9IT"></script>

    <link rel="stylesheet" href="/AdminLTE-3.0.5/plugins/toastr/toastr.min.css">
    <%= yield :javascript_includes %>
    <%= yield :stylesheet_includes %>
    <% if Rails.env.production? %>
      <!-- Google Analytics -->
      <script>
        (function(i, s, o, g, r, a, m) {
          i['GoogleAnalyticsObject'] = r;
          i[r] = i[r] || function() {
            (i[r].q = i[r].q || []).push(arguments)
          }, i[r].l = 1 * new Date();
          a = s.createElement(o),
            m = s.getElementsByTagName(o)[0];
          a.async = 1;
          a.src = g;
          m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

ga('create', 'UA-153405093-2', 'auto');
ga('send', 'pageview');
      </script>

    <% elsif Rails.env.development? %>
      <!-- Google Analytics -->
      <script>
        (function(i, s, o, g, r, a, m) {
          i['GoogleAnalyticsObject'] = r;
          i[r] = i[r] || function() {
            (i[r].q = i[r].q || []).push(arguments)
          }, i[r].l = 1 * new Date();
          a = s.createElement(o),
            m = s.getElementsByTagName(o)[0];
          a.async = 1;
          a.src = g;
          m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

ga('create', 'UA-153405093-3', 'auto');
ga('send', 'pageview');
      </script>
      <!-- End Google Analytics -->
    <% end %>
    <body>
      <%= yield %>
    </body>
</html>
